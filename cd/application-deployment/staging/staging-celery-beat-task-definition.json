{
  "family": "staging-notification-celery-beat-task",
  "executionRoleArn": "arn:aws:iam::437518843863:role/staging-notification-ecs-task-execution-role",
  "taskRoleArn": "arn:aws:iam::437518843863:role/dev-notification-api-task-role",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "notification-celery-beat",
      "image": "{will-be-replaced-by-ci}",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "staging-notification-api-log-group",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "NOTIFY_ENVIRONMENT",
          "value": "production"
        },
        {
          "name": "FLASK_APP",
          "value": "run_celery.py"
        },
        {
          "name": "NOTIFICATION_QUEUE_PREFIX",
          "value": "staging-notification-"
        },
        {
          "name": "AWS_REGION",
          "value": "us-east-2"
        },
        {
          "name": "STATSD_HOST",
          "value": "localhost"
        },
        {
          "name": "GRANICUS_URL",
          "value": "https://tms.govdelivery.com"
        },
        {
          "name": "ACCEPT_RECIPIENT_IDENTIFIERS_ENABLED",
          "value": "False"
        },
        {
          "name": "NOTIFY_EMAIL_FROM_DOMAIN",
          "value": "public.govdelivery.com"
        },
        {
          "name": "NOTIFY_EMAIL_FROM_USER",
          "value": "solutions"
        },
        {
          "name": "AWS_SES_EMAIL_FROM_DOMAIN",
          "value": "dev.api.twnotify.com"
        }
      ],
      "secrets": [
        {
          "name": "TWILIO_ACCOUNT_SID",
          "valueFrom": "arn:aws:ssm:us-east-2:437518843863:parameter/staging/notification-api/twilio/account-sid"
        },
        {
          "name": "TWILIO_AUTH_TOKEN",
          "valueFrom": "arn:aws:ssm:us-east-2:437518843863:parameter/staging/notification-api/twilio/auth-token"
        },
        {
          "name": "TWILIO_FROM_NUMBER",
          "valueFrom": "arn:aws:ssm:us-east-2:437518843863:parameter/staging/notification-api/twilio/from-number"
        },
        {
          "name": "GRANICUS_TOKEN",
          "valueFrom": "arn:aws:ssm:us-east-2:437518843863:parameter/staging/notification-api/granicus/token"
        },
        {
          "name": "SQLALCHEMY_DATABASE_URI",
          "valueFrom": "arn:aws:ssm:us-east-2:437518843863:parameter/staging/notification-api/database/uri"
        },
        {
          "name": "API_HOST_NAME",
          "valueFrom": "arn:aws:ssm:us-east-2:437518843863:parameter/staging/notification-api/api-host-name"
        },
        {
          "name": "SECRET_KEY",
          "valueFrom": "arn:aws:ssm:us-east-2:437518843863:parameter/staging/notification-api/secret-key"
        },
        {
          "name": "DANGEROUS_SALT",
          "valueFrom": "arn:aws:ssm:us-east-2:437518843863:parameter/staging/notification-api/dangerous-salt"
        }
      ],
      "command": [
        "sh",
        "-c",
        "/app/scripts/run_celery_beat.sh"
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "tags": [
    {
      "key": "Stack",
      "value": "application-infrastructure"
    },
    {
      "key": "Environment",
      "value": "staging"
    },
    {
      "key": "Team",
      "value": "va-notify"
    },
    {
      "key": "ManagedBy",
      "value": "CI"
    }
  ]
}